(* ProVerif has a single, strong model for Diffie-Hellman *)

def CDH_all_args(G, Z, g, exp, exp', mult, pCDH) {

fun exp(G,Z): G.
const g:G.

equation forall x:Z, y:Z;
         exp(exp(g,x), y) = exp(exp(g,y), x).

}

