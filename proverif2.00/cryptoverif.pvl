def CollisionResistant_hash(key, hashinput, hashoutput, hash, hashoracle, Phash) {

fun hash(key, hashinput):hashoutput.

channel ch1, ch2.
let hashoracle(k: key) =
    in(ch1, ());
    out(ch2, k).

}

